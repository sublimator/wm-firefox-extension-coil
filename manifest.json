{
  "manifest_version": 2,
  "name": "WebMonetizationDemo",
  "version": "1.0",
  "permissions": ["monetization", "storage"],
  "background": {
    "page": "background.html"
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "content_security_policy": "script-src 'self' https://jspm.dev https://unpkg.com/setimmediate@1.0.5/setImmediate.js; object-src 'self'"
}
